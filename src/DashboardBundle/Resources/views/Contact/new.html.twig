{% extends 'DashboardBundle:Common:backend2.html.twig' %}

{% block breadcrumb %}
	<li><a href="#">Contacts</a></li>
	<li><a href="#">New</a></li>
{% endblock breadcrumb%}

{% block content %}
	<div class="card card-data-grid">
		<div class="title">
			<h5> Contacts </h5>
			<div class="btn-group right">
				<a class="btn btn-small waves-dark waves-effect"><i class="mdi mdi-hardware-keyboard-arrow-left left"></i>Back</a>
				<a id="edit_action" class="btn btn-small waves-dark waves-effect"> <i class="mdi mdi-content-save left"></i> Save</a>
				<a id="edit_action" class="btn btn-small waves-dark waves-effect"> <i class="mdi mdi-action-done left"></i> Save and Close</a>
			</div>
		</div>
		<div class="content">
			<form id="formSubmit" class="form-horizontal" action="{{ path('contact_create') }}" method="post" {{ form_enctype(form) }}>
				<div class="row">
					<div class="col l6">
						{{ form_widget(form) }}
					</div>
					<div class="col l6">

					</div>
				</div>
			</form>
		</div>
	</div>
{% endblock content %}
