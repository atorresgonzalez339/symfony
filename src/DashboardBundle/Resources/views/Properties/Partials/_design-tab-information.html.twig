<div class="row" id="main-info">
  <div class="col m12">
    <form action="#!">
      <div class="card">
        <div class="title">
          <h5> Basic Information </h5>
          <div class="btn-group right">
            <a href="#!" class="waves-effect waves-dark btn btn-small">
              <i class="mdi mdi-navigation-chevron-left left"></i> Back
            </a>
            <a href="#!" class="waves-effect waves-dark btn btn-small">
              <i class="mdi mdi-content-save left"></i> Save
            </a>
          </div>
        </div>
        <div class="content" style="padding-top: 5px;padding-bottom: 5px;">
          <div class="input-field">
            {{ form_widget(property_form.name, {'attr': {'id': 'name'}}) }}
            <label for="name" class="">Name</label>
          </div>
          <div class="input-field">
            {{ form_widget(property_form.description, {'attr': {'id': 'description', 'class':'materialize-textarea'}}) }}
            <label for="description">Description</label>
          </div>
        </div>
      </div>
      <div class="card">
        <div class="title">
          <h5> Attributes </h5>
        </div>
        <div class="content" style="padding-top: 5px;padding-bottom: 5px;">
          <div class="row" style="margin-top: 5px;">
            <div class="col m6">
              <div class="input-field" style="margin-top: 0px;">
                <i class="mdi-maps-hotel prefix"></i>
                {{ form_widget(property_form.bedrooms, {'attr': {'id': 'bedrooms'}}) }}
                <label for="bedrooms" class="">Rooms</label>
              </div>
              <div class="input-field" style="margin-top: 0px;">
                <i class="mdi-action-invert-colors prefix"></i>
                {{ form_widget(property_form.bathrooms, {'attr': {'id': 'bathrooms'}}) }}
                <label for="input_baths" class="">Baths</label>
              </div>
              <div class="input-field" style="margin-top: 0px;">
                <i class="mdi-maps-directions-car prefix"></i>
                {{ form_widget(property_form.parking_spaces, {'attr': {'id': 'parking_spaces'}}) }}
                <label for="parking_spaces" class="">Parking</label>
              </div>
              <div class="input-field" style="margin-top: 3px;">
                <i class="mdi-editor-attach-money prefix"></i>
                {{ form_widget(property_form.list_price, {'attr': {'id': 'list_price'}}) }}
                <label for="list_price" class="">Price</label>
              </div>
              <div class="input-field" style="margin-top: 0px;">
                <i class="mdi-action-open-with prefix"></i>
                {{ form_widget(property_form.lotSize, {'attr': {'id': 'lotSize'}}) }}
                <label for="lotSize" class="">Size</label>
              </div>
              <div class="input-field" style="margin-top: 0px;">
                <i class="mdi-action-open-with prefix"></i>
                {{ form_widget(property_form.yearBuilt, {'attr': {'id': 'yearBuilt'}}) }}
                <label for="yearBuilt" class="">Year Built</label>
              </div>
              <div class="input-field" style="margin-top: 0px;">
                <i class="mdi-action-open-with prefix"></i>
                {{ form_widget(property_form.leaseTerm, {'attr': {'id': 'leaseTerm'}}) }}
                <label for="leaseTerm" class="">Lease Term</label>
              </div>
              <p style="margin-top: 35px;">
                <input type="checkbox" id="checkbox4" class="filled-in" checked="checked" />
                <label for="checkbox4">For rent</label>
              </p>
            </div>
            <div class="col m6">
              <div class="input-field" style="margin-top: 3px;">
                <select class="select2">
                  <option value="AK">Apartament</option>
                  <option value="HI">Condo</option>
                  <option value="HI">House</option>
                  <option value="HI">Villa</option>
                </select>
              </div>
              <br />
              <div class="input-field" style="margin-top: 0px;">
                {{ form_widget(property_form.address, {'attr': {'id': 'address'}}) }}
                <label for="address" class="">Address</label>
              </div>
              <div class="input-field" style="margin-top: 0px;">
                {{ form_widget(property_form.unit, {'attr': {'id': 'unit'}}) }}
                <label for="unit" class="">Unit</label>
              </div>
              <div class="input-field" style="margin-top: 0px;">
                {{ form_widget(property_form.city, {'attr': {'id': 'city'}}) }}
                <label for="city" class="">City</label>
              </div>
              <div class="input-field" style="margin-top: 0px;">
                {{ form_widget(property_form.state, {'attr': {'id': 'state'}}) }}
                <label for="state" class="">State</label>
              </div>
              <div class="input-field" style="margin-top: 0px;">
                {{ form_widget(property_form.country, {'attr': {'id': 'country'}}) }}
                <label for="country" class="">Country</label>
              </div>
              <div class="input-field" style="margin-top: 0px;">
                {{ form_widget(property_form.postalCode, {'attr': {'id': 'postalCode'}}) }}
                <label for="postalCode" class="">Postal Code</label>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="card">
        <div class="title">
          <h5> Location </h5>
        </div>
        <div class="content" style="padding-top: 5px;padding-bottom: 5px;">
          <div class="row" style="margin-top: 5px">
            <div class="col m12" style="padding-bottom: 5px;">
              <input id="pac-input" class="controls" type="text" placeholder="Enter a location">
              <div id="map-canvas"></div>
            </div>
          </div>
          <div class="row" style="margin-top: 5px">
            <div class="col m6">
              <div class="input-field">
                <i class="mdi-maps-place prefix"></i>
                {{ form_widget(property_form.lat, {'attr': {'id': 'lat'}}) }}
                <label for="lat" class="">Latitude</label>
              </div>
            </div>
            <div class="col m6">
              <div class="input-field">
                <i class="mdi-maps-place prefix"></i>
                {{ form_widget(property_form.lng, {'attr': {'id': 'lng'}}) }}
                <label for="lng" class="">Lontgitude</label>
              </div>
            </div>
          </div>
        </div>
      </div>
    </form>
  </div>
</div>